{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AppSettings": {

    "AstreaClient": {
      "Version": "#{AppVersion}#",
      "Url": "#{Url}#",
      "RetryPeriodInMin": "#{RetryPeriodInMin}#",
      "RiskThreshold": "#{RiskThreshold}#",
      "AcceptableTransactionTypes": [ "#{AcceptableTransactionTypes}#" ]
    },

    "KafkaProducer": {
      "Topic": "#{Topic}#",
      "BootstrapServers": "#{BootstrapServers}#",
      "SslCaLocation": "#{SslCaLocation}#",
      "SslCertificateLocation": "#{SslCertificateLocation}#",
      "SslKeyLocation": "#{SslKeyLocation}#",
      "SslKeyPassword": "#{SslKeyPassword}#",
      "MessageSendMaxRetries": "#{MessageSendMaxRetries}#",
      "RetryBackoffMs": "#{RetryBackoffMs}#",
      "MaxInFlight": "#{MaxInFlight}#"
    },
    "KafkaConsumer": {
      "Topic": "#{Topic}#",
      "BootstrapServers": "#{BootstrapServers}#",
      "SslCaLocation": "#{SslCaLocation}#",
      "SslCertificateLocation": "#{SslCertificateLocation}#",
      "SslKeyLocation": "#{SslKeyLocation}#",
      "SslKeyPassword": "#{SslKeyPassword}#"
    },

    "MqConfigurations": {
      "ReaderConfig": {
        "MqChannel": "#{MqChannel}#",
        "MqQueueManagerName": "#{MqQueueManagerName}#",
        "MqQueueName": "#{MqQueueName}#",
        "MqHostname": "#{MqHostname}#",
        "MqPort": "#{MqPort}#",
        "MqSslPath": "#{MqSslPath}#",
        "MqKeyRepo": "#{MqKeyRepo}#",
        "MqSslCipher": "#{MqSslCipher}#",
        "MqUsername": "#{MqUsername}#",
        "MqPassword": "#{MqPassword}#"
      },
      "WriterConfig": {
        "MqChannel": "#{MqChannel}#",
        "MqHostname": "#{MqHostname}#",
        "MqKeyRepo": "#{MqKeyRepo}#",
        "MqPassword": "#{MqPassword}#",
        "MqPort": "#{MqPort}#",
        "MqQueueManagerName": "#{MqQueueManagerName}#",
        "MqQueueName": "#{MqQueueName}#",
        "MqSslCipher": "#{MqSslCipher}#",
        "MqSslPath": "#{MqSslPath}#",
        "MqUsername": "#{MqUsername}#"
      }
    }
  }
}
